{
  "name": "pulsewisdom",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:mobile": "yarn workspace @pulsewisdom/mobile start",
    "dev:web": "yarn workspace @pulsewisdom/web start",
